{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}
    <h1>{{ client.secondName }} {{ client.firstName }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ client.id }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_client_index') }}">Вернуться к списку</a>

    <a href="{{ path('app_client_edit', {'id': client.id}) }}">Редактировать</a>

{% endblock %}
